<?xml version="1.0" encoding="utf-8" ?>
<widget id="com.mendix.widget.custom.kanban.kanbanWidget" needsEntityContext="true" 
    xmlns="http://www.mendix.com/widget/1.0/">
    <name>PlanningBoard</name>
    <description>Widget for visual planning and task management.</description>
    <icon>
</icon>
    <properties>
        <property key="board" type="entity">
            <caption>Board*</caption>
            <category>1) Board</category>
            <description>Board entity. The widget has to be placed in dataview which has Board as context.</description>
        </property>
        <property key="boardOpenLabels" type="attribute" entityProperty="board" required="false">
            <caption>Labels opened/closed states</caption>
            <category>1) Board</category>
            <description>Boolean value for the state of labels. If true, all labels will be opened and otherwise.</description>
            <attributeTypes>
                <attributeType name="Boolean"/>
            </attributeTypes>
        </property>
        <property key="boardReadonly" type="attribute" entityProperty="board" required="false">
            <caption>Board&#39;s readonly/editable mode</caption>
            <category>1) Board</category>
            <description>Boolean value for readonly/edit mode of the board. If true, users are able to edit the Board and otherwise.</description>
            <attributeTypes>
                <attributeType name="Boolean"/>
            </attributeTypes>
        </property>
        <property key="offlineSupported" type="boolean" defaultValue="false">
            <caption>Offline supported</caption>
            <category>1) Board</category>
            <description>Boolean value for offline mode of the board. If you are using widget offline supported, please select No.</description>
        </property>
        <property key="list" type="entity">
            <caption>List*</caption>
            <category>2) List</category>
            <description>List entity</description>
        </property>
        <property key="listName" type="attribute" entityProperty="list">
            <caption>List&#39;s Name*</caption>
            <category>2) List</category>
            <description>Name of the List, will be displayed as title of the list.</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>
        <property key="listOrder" type="attribute" entityProperty="list">
            <caption>List&#39;s Order*</caption>
            <category>2) List</category>
            <description>Horizontal order of the List in Board, from right to left.</description>
            <attributeTypes>
                <attributeType name="Decimal"/>
            </attributeTypes>
        </property>
        <property key="listEditForm" type="form" required="false">
            <caption>List&#39;s Edit form</caption>
            <category>2) List</category>
            <description>List edit form</description>
        </property>
        <property key="listBoardAssociation" type="entity" entityProperty="list" isPath="yes" pathType="reference" required="true">
            <caption>List-Board relation*</caption>
            <category>2) List</category>
            <description>Association from List to Board</description>
        </property>
        <property key="allowRemoveList" type="boolean" defaultValue="true">
            <caption>Allow remove list</caption>
            <category>2) List</category>
            <description>Boolean value for allow to remove lists on the board. If false, users cannot to using remove list action in board.</description>
        </property>
        <property key="allowAddList" type="boolean" defaultValue="true">
            <caption>Allow add list</caption>
            <category>2) List</category>
            <description>Boolean value for allow to add lists on the board. If false, users cannot to using add list action in board.</description>
        </property>
        <property key="card" type="entity">
            <caption>Card*</caption>
            <category>3) Card</category>
            <description>Card Entity</description>
        </property>
        <property key="cardName" type="attribute" entityProperty="card">
            <caption>Card&#39;s Name*</caption>
            <category>3) Card</category>
            <description>Name of Card</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>
        <property key="cardDescription" type="attribute" entityProperty="card">
            <caption>Card&#39;s Desciption</caption>
            <category>3) Card</category>
            <description>Description of Card</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>
        <property key="cardOrder" type="attribute" entityProperty="card">
            <caption>Card&#39;s Order*</caption>
            <category>3) Card</category>
            <description>Vertical Order of Card in List, from top to bottom</description>
            <attributeTypes>
                <attributeType name="Decimal"/>
            </attributeTypes>
        </property>
        <property key="cardDueDate" type="attribute" entityProperty="card" required="false">
            <caption>Card&#39;s DueDate</caption>
            <category>3) Card</category>
            <description>Due date of Card</description>
            <attributeTypes>
                <attributeType name="DateTime"/>
            </attributeTypes>
        </property>
        <!-- <property key="cardState" type="attribute" entityProperty="card" required="false"><caption>Card State</caption><category>3) Card</category><description>State of the Card</description><attributeTypes><attributeType name="Enum"/></attributeTypes></property> -->
        <!-- <property key="cardStartDate" type="attribute" entityProperty="card" required="false"><caption>Card StartDate</caption><category>3) Card</category><description>StartDate of the Card</description><attributeTypes><attributeType name="DateTime"/></attributeTypes></property> -->
        <property key="cardEditForm" required="false" type="form">
            <caption>Card&#39;s Edit form</caption>
            <category>3) Card</category>
            <description>Edit form will be opened if user clicks on card</description>
        </property>
        <property key="cardListAssociation" type="entity" entityProperty="card" isPath="yes" pathType="reference">
            <caption>Card-List relation*</caption>
            <category>3) Card</category>
            <description>Assocation from Card to List</description>
        </property>
        <property key="allowRemoveCard" type="boolean" defaultValue="true">
            <caption>Allow remove card</caption>
            <category>3) Card</category>
            <description>Boolean value for allow to remove cards on the board. If false, users cannot to using remove card action in board.</description>
        </property>
        <property key="allowAddCard" type="boolean" defaultValue="true">
            <caption>Allow add card</caption>
            <category>3) Card</category>
            <description>Boolean value for allow to add cards on the board. If false, users cannot to using add card action in board.</description>
        </property>
        <property key="comment" type="entity" required="false">
            <caption>Comment</caption>
            <category>4) Comment</category>
            <description>Comment of Card</description>
        </property>
        <property key="commentCardAssociation" type="entity" entityProperty="comment" isPath="yes" pathType="reference" required="false">
            <caption>Comment-Card relation</caption>
            <category>4) Comment</category>
            <description>Assocation from Comment to Card</description>
        </property>
        <!-- <property key="commentText" type="attribute" entityProperty="comment" required="false"><caption>Comment Text</caption><category>4) Comment</category><description>Text of the Comment</description><attributeTypes><attributeType name="String"/></attributeTypes></property> -->
        <property key="label" type="entity" required="false">
            <caption>Label</caption>
            <category>5) Label</category>
            <description>Labels assigned to card to add more information to Card. For example, label can be DANGER(red), SAFE(green) or RESTRICTED(black)</description>
        </property>
        <property key="labelName" type="attribute" entityProperty="label" required="false">
            <caption>Label&#39;s Name</caption>
            <category>5) Label</category>
            <description>Name of the Label</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>
        <property key="labelColor" type="attribute" entityProperty="label" required="false">
            <caption>Label&#39;s Color</caption>
            <category>5) Label</category>
            <description>Color of Label</description>
            <attributeTypes>
                <attributeType name="Enum"/>
            </attributeTypes>
        </property>
        <property key="labelCardAssociation" type="entity" entityProperty="label" isPath="yes" pathType="referenceSet" required="false">
            <caption>Label-Card relation</caption>
            <category>5) Label</category>
            <description>Assocation from Label to Card</description>
        </property>
        <property key="workList" type="entity" required="false">
            <caption>WorkList (SubTasks)</caption>
            <category>6) WorkList</category>
            <description>List of Works(SubTask) in Card. We use this one to determine number of subtask to show on Card.</description>
        </property>
        <!-- <property key="workListName" type="attribute" entityProperty="workList" required="false"><caption>Worklist Name</caption><category>6) WorkList</category><description>Name of the WorkList</description><attributeTypes><attributeType name="String"/></attributeTypes></property> -->
        <property key="workListProgressRate" type="attribute" entityProperty="workList" required="false">
            <caption>Work&#39;s ProgressRate</caption>
            <category>6) WorkList</category>
            <description>Progress rate of Worklist</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>
        <property key="workListCardAssociation" type="entity" entityProperty="workList" isPath="yes" pathType="reference" required="false">
            <caption>WorkList-Card relation</caption>
            <category>6) WorkList</category>
            <description>Assocation from WorkList to Card</description>
        </property>
        <!-- <property key="work" type="entity" required="false"><caption>Work</caption><category>7) Work</category><description>Work in the WorkList</description></property> -->
        <!-- <property key="workName" type="attribute" entityProperty="work" required="false"><caption>Work Name</caption><category>7) Work</category><description>Name of the Work</description><attributeTypes><attributeType name="String"/></attributeTypes></property> -->
        <!-- <property key="workState" type="attribute" entityProperty="work" required="false"><caption>Work State</caption><category>7) Work</category><description>State of the Work</description><attributeTypes><attributeType name="Enum"/></attributeTypes></property> -->
        <property key="attachment" type="entity" required="false">
            <caption>Attachment</caption>
            <category>7) Attachment</category>
            <description>Attachment on Card</description>
        </property>
        <property key="attachmentFileId" type="attribute" entityProperty="attachment" required="false">
            <caption>Attachment File Id</caption>
            <category>7) Attachment</category>
            <description>FileID of attached file</description>
            <attributeTypes>
                <attributeType name="AutoNumber"/>
            </attributeTypes>
        </property>
        <property key="attachmentCardAssociation" type="entity" entityProperty="attachment" isPath="yes" pathType="reference" required="false">
            <caption>Attachment-Card relation</caption>
            <category>7) Attachment</category>
            <description>Assocation from Attachment to Card</description>
        </property>
        <!-- <property key="attachmentText" type="attribute" entityProperty="attachment" required="false"><caption>Attachment Text</caption><category>7) Attachment</category><description>link or note</description><attributeTypes><attributeType name="String"/></attributeTypes></property> -->
        <!-- <property key="document" type="entity" required="false"><caption>Document</caption><category>7) Attachment</category><description>file document</description></property> -->
        <property key="avatar" type="entity" required="false">
            <caption>Avatar</caption>
            <category>8) Member</category>
            <description>Avatar of assignees</description>
        </property>
        <property key="account" type="entity" required="false">
            <caption>Assignee</caption>
            <category>8) Member</category>
            <description>Assignee</description>
        </property>
        <property key="accountFullName" type="attribute" entityProperty="account" required="false">
            <caption>Assignee&#39;s FullName</caption>
            <category>8) Member</category>
            <description>Fullname of assignee</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>
        <property key="memberCardAssociation" type="entity" entityProperty="account" isPath="yes" pathType="referenceSet" required="false">
            <caption>Assignee-Card relation</caption>
            <category>8) Member</category>
            <description>Assocation from Assignee to Card</description>
        </property>
