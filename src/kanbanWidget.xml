        <!-- <property key="boardMemberAssociation" type="entity" entityProperty="board" isPath="yes" pathType="referenceSet" required="false"><caption>Board-Member relation</caption><category>1) Board</category><description>relation between board and account</description></property>   -->
        <!-- <property key="mentionCardCommentAssociation" type="entity" entityProperty="card" isPath="yes" pathType="reference" required="false"><caption>MentionCard-Comment relation</caption><category>3) Card</category><description>relation between mention card and comment</description></property>  -->
        <!-- <property key="commentOwnerAssociation" type="entity" entityProperty="comment" isPath="yes" pathType="reference" required="false"><caption>Comment-Owner relation</caption><category>4) Comment</category><description>relation between comment and its owner</description></property>   -->
        <!-- <property key="workWorkListAssociation" type="entity" entityProperty="work" isPath="yes" pathType="reference" required="false"><caption>Work-WorkList relation</caption><category>7) Work</category><description>relation between work and work list</description></property> -->
        <!-- <property key="attachmentCommentAssociation" type="entity" entityProperty="attachment" isPath="yes" pathType="reference" required="false"><caption>Attachment-Comment relation</caption><category>7) Attachment</category><description>relation between attachment and comment</description></property> -->
        <!-- <property key="documentAttachmentAssociation" type="entity" entityProperty="document" isPath="yes" pathType="reference" required="false"><caption>Document-Attachment relation</caption><category>7) Attachment</category><description>relation between document and attachment</description></property> -->
        <!-- <property key="subscriberCardAssociation" type="entity" entityProperty="account" isPath="yes" pathType="referenceSet" required="false"><caption>Subscriber-Card relation</caption><category>8) Member</category><description>relation between subscriber and card</description></property> -->
        <!-- <property key="viewerBoardAssociation" type="entity" entityProperty="account" isPath="yes" pathType="referenceSet" required="false"><caption>Viewer-Board relation</caption><category>8) Member</category><description>relation between viewer and board</description></property> -->
        <!-- <property key="tagMemberCommentAssociation" type="entity" entityProperty="account" isPath="yes" pathType="referenceSet" required="false"><caption>TagMember-Comment relation</caption><category>8) Member</category><description>relation between tag account and comment</description></property>  -->
        <property key="accountAvatarAssociation" type="entity" entityProperty="account" isPath="yes" pathType="reference" required="false">
            <caption>Assignee-Avatar relation</caption>
            <category>8) Member</category>
            <description>Assocation from Assignee to Avatar</description>
        </property>
    </properties>
</widget>